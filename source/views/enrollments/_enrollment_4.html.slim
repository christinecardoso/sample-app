          .counter
            | 4
        li.text-center
          i.icon.icon-review data-html="true" data-original-title="&lt;strong&gt;REVIEWS&lt;/strong&gt;&lt;br&gt;6/6" data-toggle="tooltip" title=""
          .counter
            | 6
        li.text-center
          i.icon.icon-one-on-one data-html="true" data-original-title="&lt;strong&gt;ONE ON ONES&lt;/strong&gt;&lt;br&gt;4/4" data-toggle="tooltip" title=""
          .counter
            | 4
    .course-announcement
      .announcement-text
        h5 Final Exam
        p Available soon!
  .enrollment-footer
    .course-grade-details
      .detail-tab
        | YOUR GRADE
        = succeed "/" do
          span.incorrect 1
        span.correct 37
        a.btn-toggle.collapsed data-target="#grading-details-panel-3" data-toggle="collapse" href="#"
          | Details
          i.icon-small
      .detail-tab
