          .counter
            | 1
        li.text-center
          i.icon.icon-problem data-html="true" data-original-title="&lt;strong&gt;PROBLEMS&lt;/strong&gt;&lt;br&gt;0/5" data-toggle="tooltip" title=""
          .counter
            | 0
        li.text-center
          i.icon.icon-demonstration data-html="true" data-original-title="&lt;strong&gt;DEMONSTRATIONS&lt;/strong&gt;&lt;br&gt;11/11" data-toggle="tooltip" title=""
          .counter
            | 11
        li.text-center
