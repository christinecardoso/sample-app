      ul
        li.text-center
          i.icon.icon-video data-html="true" data-original-title="&lt;strong&gt;VIDEOS&lt;/strong&gt;&lt;br&gt;0/6" data-toggle="tooltip" title=""
          .counter
            | 0
        li.text-center
          i.icon.icon-exercise data-html="true" data-original-title="&lt;strong&gt;EXERCISES&lt;/strong&gt;&lt;br&gt;0/5" data-toggle="tooltip" title=""
          .counter
            | 0
        li.text-center
          i.icon.icon-problem data-html="true" data-original-title="&lt;strong&gt;PROBLEMS&lt;/strong&gt;&lt;br&gt;0/1" data-toggle="tooltip" title=""
