.enrollment.enrolled
  .enrollment-header
    .course-thumbnail
      = image_tag 'courses/icons/quantum-mechanics-i.png'
    h3.title.pull-left
      | Quantum Mechanics, Part I
    .continue.pull-right
      a.btn.btn-large.btn-action.btn-red href="#"
        | Start
        i.icon.icon-arrow-thin-right
  .enrollment-body
    .course-progress
      h4
        | Course Progress
        i.icon.icon-circle-i data-html="true" data-original-title="&lt;strong&gt;Certification Requirements&lt;/strong&gt;&lt;br&gt;75% of completion&lt;br&gt;+ 75% of points" data-toggle="tooltip" title=""
      .progress-bar
        .progress
          .bar style="width:0%"
          .percentage style="left:0%"
            .rule
            .percent
              | 0
              span.symbol %
